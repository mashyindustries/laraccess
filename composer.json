{
    "name": "mashyindustries/laraccess",
    "description": "Roles, Inheritence & Permissions for Laravel 5.4",
    "keywords": [
        "mashyindustries",
        "laravel",
        "laraccess",
        "permissions",
        "acl",
        "security",
        "roles",
        "inheritance"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Albert Mashy",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.6.0",
        "laravel/framework": "5.4.*",
        "illuminate/contracts": "5.4.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "orchestra/testbench": "^3.1.4"
    },
    "autoload": {
        "psr-4": {
            "Mashy\\Laraccess\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mashy\\Laraccess\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev"
    
}
